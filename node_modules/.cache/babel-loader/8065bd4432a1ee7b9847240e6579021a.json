         <v-img\n              class=\"white--text align-end\"\n              height=\"275px\"\n              :src=\"category.icons[0].url\"\n            >\n              <v-card-title\n                class=\"category--title\"\n                @click=\"navigateToDetail(category.id)\"\n              >\n                {{ category.name }}\n              </v-card-title>\n            </v-img>\n          </v-card>\n        </v-col>\n      </v-row>\n    </v-container>\n  </div>\n</template>\n\n<script>\nimport router from \"@/router\";\nimport { getGenres } from \"@/services\";\n\nexport default {\n  name: \"Category\",\n  data: () => ({\n    categories: [],\n  }),\n  methods: {\n    navigateToDetail(id) {\n      console.log(id);\n      router.push({ name: \"CategoryDetail\", params: { id } });\n    },\n    async getGen() {\n      const { categories } = await getGenres();\n\n      this.categories = categories.items;\n      return categories;\n    },\n  },\n  mounted() {\n    this.getGen();\n  },\n};\n</script>\n\n<style scoped>\n.category--title {\n  transition: all 0.3s ease;\n  user-select: none;\n}\n\n.category--title:hover {\n  transition: all 0.3s ease;\n  color: #f8bbd0;\n}\n</style>\n"],"sourceRoot":"src/views/Category"}]}