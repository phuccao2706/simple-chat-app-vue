 class=\"md-4\"\n        >\n          <v-card\n            class=\"mx-auto\"\n            min-width=\"275px\"\n            max-width=\"275px\"\n            @click=\"navigateToDetail(playlist.id)\"\n          >\n            <v-img\n              class=\"white--text align-end\"\n              height=\"275px\"\n              :src=\"playlist.images[0].url\"\n            >\n              <v-card-title class=\"category--title\">\n                {{ playlist.name }}\n              </v-card-title>\n            </v-img>\n          </v-card>\n        </v-col>\n      </v-row>\n    </v-container>\n  </div>\n</template>\n\n<script>\nimport router from \"@/router\";\nimport { getCategoryPlaylists } from \"@/services\";\n\nexport default {\n  Name: \"CategoryDetail\",\n  data: () => ({\n    playlists: [],\n  }),\n  methods: {\n    navigateToDetail(id) {\n      router.push({ name: \"Playlist\", params: { id } });\n    },\n    async _getCategoryPlaylists(id) {\n      const { playlists } = await getCategoryPlaylists(id);\n      this.playlists = playlists.items;\n    },\n  },\n\n  mounted() {\n    const id = router.currentRoute.params.id;\n    this._getCategoryPlaylists(id);\n  },\n};\n</script>\n"],"sourceRoot":"src/views/Category"}]}